<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

<h2>プロフィール編集</h2>
<%= form_with model: @user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName" %>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  <label for="inputIntroduction">Introduction</label>
  <%= f.text_area :introduction, id:"inputIntroduction" %>
  <%= f.submit "変更を保存" %>
<% end %>